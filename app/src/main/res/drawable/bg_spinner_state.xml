<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="?attr/colorSurface"/>
            <corners android:radius="14dp"/>
            <stroke
                android:width="1dp"
                android:color="#33000000"/>
